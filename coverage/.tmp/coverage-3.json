r\n  let repeatedNumber = 1;\r\n  let majorityNumber: string = \"none\";\r\n\r\n  for (const number of numbersArray) {\r\n    coutingEvenNumbers(number);\r\n    checkingforHighestRepeatedNumber(number);\r\n  }\r\n\r\n  if (repeatedNumber === 1) {\r\n    const EvenMajority = evenNumberCount > numbersArray.length / 2;\r\n    return EvenMajority ? \"Even Majority\" : \"Odd Majority\";\r\n  } else {\r\n    return majorityNumber;\r\n  }\r\n\r\n  function checkingforHighestRepeatedNumber(number: number) {\r\n    let count = 0;\r\n    for (let i = 0; i < numbersArray.length; i++) {\r\n      if (number === numbersArray[i]) {\r\n        count++;\r\n      }\r\n      if (count > repeatedNumber) {\r\n        repeatedNumber = count;\r\n        majorityNumber = numbersArray[i].toString();\r\n        i++;\r\n      }\r\n    }\r\n  }\r\n\r\n  function coutingEvenNumbers(number: number) {\r\n    if (number % 2 === 0) {\r\n      evenNumberCount++;\r\n    }\r\n  }\r\n};\r\n"],"mappings":"AAAO,aAAM,oBAAoB,CAAC,iBAA2B;AAC3D,MAAI,kBAAkB;AACtB,MAAI,iBAAiB;AACrB,MAAI,iBAAyB;AAE7B,aAAW,UAAU,cAAc;AACjC,uBAAmB,MAAM;AACzB,qCAAiC,MAAM;AAAA,EACzC;AAEA,MAAI,mBAAmB,GAAG;AACxB,UAAM,eAAe,kBAAkB,aAAa,SAAS;AAC7D,WAAO,eAAe,kBAAkB;AAAA,EAC1C,OAAO;AACL,WAAO;AAAA,EACT;AAEA,WAAS,iCAAiC,QAAgB;AACxD,QAAI,QAAQ;AACZ,aAAS,IAAI,GAAG,IAAI,aAAa,QAAQ,KAAK;AAC5C,UAAI,WAAW,aAAa,CAAC,GAAG;AAC9B;AAAA,MACF;AACA,UAAI,QAAQ,gBAAgB;AAC1B,yBAAiB;AACjB,yBAAiB,aAAa,CAAC,EAAE,SAAS;AAC1C;AAAA,MACF;AAAA,IACF;AAAA,EACF;AAEA,WAAS,mBAAmB,QAAgB;AAC1C,QAAI,SAAS,MAAM,GAAG;AACpB;AAAA,IACF;AAAA,EACF;AACF;","names":[]},"_coverageSchema":"1a1c01bbd47fc00a2c39e90264f33305004495a9","hash":"7425972dcc1203c3b362fa91f655308332bf15ce"}}